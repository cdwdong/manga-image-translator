{
"filter_text": null,
  "render": {
    "renderer": "manga2eng_pillow",
    "alignment": "auto",
    "disable_font_border": false,
    "font_size_offset": 0,
    "font_size_minimum": -1,
    "direction": "auto",
    "uppercase": false,
    "lowercase": false,
    "gimp_font": "Sans-serif",
    "no_hyphenation": false,
    "font_color": ":FFFFFF",
    "line_spacing": null,
    "font_size": null,
    "rtl": false
  },
  "upscale": {
    "upscaler": "esrgan",
    "revert_upscaling": true,
    "upscale_ratio": 2
  },
  "translator": {
    "translator": "gemini",
